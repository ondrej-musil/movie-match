{
  "expo": {
    "name": "2-movie-match",
    "slug": "2-movie-match",
    "scheme": "movie-match-2",
    "version": "1.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#D32F2F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "cz.moviematch",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to provide location-based features.",
        "NSContactsUsageDescription": "This app needs access to your contacts to enhance your experience.",
        "NSCameraUsageDescription": "This app needs access to your camera to allow you to take photos or videos.",
        "NSCalendarsUsageDescription": "This app needs access to your calendar to add and manage events."
      },
      "buildNumber": "44"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "versionCode": 44,
      "package": "cz.moviematch"
    },
    "extra": {
      "eas": {
        "projectId": "7e47f012-9bde-460a-8c15-a63eb5ed0302"
      },
      "expo": {
        "doctor": {
          "reactNativeDirectoryCheck": {
            "listUnknownPackages": false
          }
        }
      }
    },
    "plugins": [
      "expo-build-properties",
      "expo-font",
      "expo-mail-composer",
      "expo-sqlite",
      "expo-video",
      "expo-web-browser",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "movie-match.cz",
          "organization": "movie-matchcz",
          "disableAutoUpload": true,
          "uploadSourceMaps": false
        }
      ]
    ]
  }
}
